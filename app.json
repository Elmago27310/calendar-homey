          "en": "Event starts",
          "no": "En avtale starter"
          "nl": "Event start"
        },
        "tokens": [
          {
            "name": "event_name",
            "type": "string",
            "title": {
              "en": "Title",
              "no": "Tittel"
              "nl": "Titel"
            },
            "example": {
              "en": "Pick up in kindergarden",
              "no": "Hente i barnehagen"
              "nl": "Kinderen ophalen"
            }
          },
          {
            "name": "event_description",
            "type": "string",
            "title": {
              "en": "Description",
              "no": "Beskrivelse"
              "nl": "Omschrijving"
            },
            "example": {
              "en": "Pick up kids in kindergarden",
              "no": "Hente barna fra barnehagen"
              "nl": "Haal de kinderen van school op"
            }
          },
          {
            "name": "event_location",
            "type": "string",
            "title": {
              "en": "Location",
              "no": "Sted"
              "nl": "Lokatie"
            },
            "example": {
              "en": "Kindergarden street, 30",
              "no": "Barnehagegata, 30"
              "nl": "Schoolstraat 30"
            }
          },
          {
            "name": "event_duration_readable",
            "type": "string",
            "title": {
              "en": "Duration",
              "no": "Varighet"
              "nl": "Looptijd"
            },
            "example": {
              "en": "1.5 hour",
              "no": "1.5 time"
              "nl": "1.5 uur"
            }
          },
          {
            "name": "event_duration",
