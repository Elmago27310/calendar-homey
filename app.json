{
  "id": "no.runely.calendar",
  "version": "0.0.1",
  "compatibility": ">=2.0.0",
  "sdk": 2,
  "name": {
    "en": "IcalCalendar"
  },
  "description": {
    "en": "Read ical"
  },
  "category": [
    "tools"
  ],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Rune Moskvil Lyngås",
    "email": "runely85@gmail.com"
  },
  "contributors": {
    "developers": [
      {
        "name": "Rune Moskvil Lyngås",
        "email": "runely85@gmail.com"
      }
    ]
  },
  "tags": {
    "en": [
      "calendar",
      "ics",
      "ical"
    ]
  },
  "bugs": {
    "url": "https://github.com/runely/calendar-homey/issues"
  },
  "source": "https://github.com/runely/calendar-homey",
  "flow": {
    "conditions": [
      {
        "id": "event_in",
        "title": {
          "en": "Event !{{is|isn't}} ongoing in..."
        },
        "args": [
          {
            "name": "when",
            "type": "dropdown",
            "values": [
              {
                "id": "5",
                "label": {
                  "en": "5 minutes"
                }
              },
              {
                "id": "10",
                "label": {
                  "en": "10 minutes"
                }
              },
              {
                "id": "15",
                "label": {
                  "en": "15 minutes"
                }
              },
              {
                "id": "30",
                "label": {
                  "en": "30 minutes"
                }
              }
            ]
          },
          {
            "name": "event",
            "type": "autocomplete",
            "placeholder": {
              "en": "Select event..."
            }
          }
        ]
      },
      {
        "id": "event_ongoing",
        "title": {
          "en": "Event !{{is|isn't}} ongoing"
        },
        "args": [
          {
            "name": "event",
            "type": "autocomplete",
            "placeholder": {
              "en": "Select event..."
            }
          }
        ]
      }
    ]
  }
}